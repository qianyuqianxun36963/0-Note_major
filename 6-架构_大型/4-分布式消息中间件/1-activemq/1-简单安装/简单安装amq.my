	<li>/**&nbsp;</li>
	<li>&nbsp;*&nbsp;@author&nbsp;&nbsp;</li>
	<li>&nbsp;*&nbsp;@company&nbsp;&nbsp;</li>
	<li>&nbsp;*&nbsp;@date&nbsp;</li>
	<li>&nbsp;*/&nbsp;&nbsp;</li>
	<li>public&nbsp;class&nbsp;Receiver&nbsp;{&nbsp;&nbsp;</li>
	<li>&nbsp;&nbsp;</li>
	<li>&nbsp;&nbsp;</li>
	<li>&nbsp;&nbsp;</li>
